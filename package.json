{
  "name": "SportStore",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A trainig project for react",
  "sideEffects": false,
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm-run-all --parallel start:mockserver start:app",
    "start:mockserver": "node mockServer.js",
    "start:app": "webpack-dev-server --mode development",
    "start:wdio": "wdio",
    "start:wdio:dev": "wdio --watch",
    "test": "jest",
    "test:integration": "npm-run-all --parallel start start:wdio",
    "test:integration:dev": "npm-run-all --parallel start start:wdio:dev",
    "precommit": "lint-staged"
  },
  "keywords": [],
  "author": "olexandr",
  "dependencies": {
    "axios": "0.18.0",
    "babel-jest": "23.6.0",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "expect": "23.6.0",
    "identity-obj-proxy": "3.0.0",
    "immutable": "3.8.2",
    "json-server": "0.14.0",
    "npm-run-all": "4.1.3",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-flexbox-grid": "2.1.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "recompose": "0.30.0",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
    "redux-immutable": "4.0.0",
    "redux-promise-middleware": "5.1.1",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "styled-components": "3.4.9",
    "wdio-jasmine-framework": "0.3.6",
    "wdio-screenshots-cleanup-service": "0.0.7",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "0.1.5"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "1.7.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-airbnb": "2.6.0",
    "clean-webpack-plugin": "0.1.19",
    "directory-named-webpack-plugin": "4.0.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.1",
    "jest": "23.6.0",
    "jest-webpack-resolver": "0.3.0",
    "lint-staged": "7.2.2",
    "prettier-eslint-cli": "4.7.1",
    "webdriverio": "4.13.2",
    "webpack": "4.18.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-manifest-plugin": "2.0.4"
  },
  "lint-staged": {
    "*.{js,json,jsx}": [
      "prettier --print-width 120 --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jestWebpackResolver": {
    "silent": true,
    "webpackConfig": "./webpack.config.js"
  }
}
